<div class="ratings">
  {{#each this}}
  <section class="rating">
    <div class="meta" style="inline-flex">
      {{user.username}} on {{format_date created_at}}

      {{#if rating_score.value}}
      {{#each rating_score.value}}
      <span class="fa fa-cookie-bite"></span>
      {{/each}}
      {{else}}
      <span class="fa fa-cookie"></span>
      <span class="fa fa-cookie"></span>
      <span class="fa fa-cookie"></span>
      <span class="fa fa-cookie"></span>
      <span class="fa fa-cookie"></span>
      {{/if}}
    </div>
    <div class="text">
      {{rating_comment}}
    </div>
  </section>
  {{/each}}
</div>